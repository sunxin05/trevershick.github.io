---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>{{site.baseurl}}/ced</loc>
	</url>
	<url>
		<loc>{{site.baseurl}}/certs</loc>
	</url>
	<url>
		<loc>{{site.baseurl}}/releases</loc>
	</url>
	<url>
		<loc>{{site.baseurl}}/about</loc>
	</url>
	<url>
		<loc>{{site.baseurl}}/tags</loc>
	</url>
	<url>
		<loc>{{site.baseurl}}/series</loc>
	</url>
	{% for post in site.posts %}
	  <url>
		<loc>{{ post.url | prepend: site.baseurl | prepend: site.url }}</loc>
	  </url>
	{% endfor %}
</urlset>
